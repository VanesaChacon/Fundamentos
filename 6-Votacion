#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{	int edad;
    cout<<"Ingrese la Edad: ";
	cin>>edad;
	if(edad>=18)
		cout<<"Usted puede votar";
	else
		cout<<"Usted no puede votar";

	_getch();
}
