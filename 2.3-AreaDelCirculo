// 2.3-AreaDelCirculo.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"

using namespace std;

void main ()
{	float area, radio, pi=3.1415;
    cout<<"Ingrese el radio: ";
	cin>>radio;
	area= pi*(pow(radio,2));
	cout<<"Es area es:"<<area;
	cout<<endl;
	_getch ();
}
